{
  "extends": "../../tsconfig.json",
  "compilerOptions": {
    "target": "es5",
    "lib": ["dom", "dom.iterable", "esnext"],
    "allowJs": true,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "Node",
    "types": ["../server/src/express.d.ts"],
    "plugins": [
      {
        "name": "next",
      },
    ],
    "jsx": "preserve",
  },
  "include": [
    "next-env.d.ts",
    "**/**/*.ts",
    "**/**/*.tsx",
    ".next/types/**/*.ts",
  ],
  "exclude": ["node_modules"],
}